# Database Configuration
DB_USERNAME=root
DB_PASSWORD=your_db_password
DB_NAME=TripToNDB

# Application Configuration
SERVER_PORT=8080

# JPA Configuration
JPA_DDL_AUTO=update
JPA_SHOW_SQL=false

# Gemini AI Configuration
GEMINI_ENABLED=true
GEMINI_API_KEY=your_gemini_api_key_here

# Docker Network (MySQL 컨테이너가 속한 네트워크 이름)
DOCKER_NETWORK=bridge
